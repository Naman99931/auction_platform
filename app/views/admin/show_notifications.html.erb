<h3>Notifications</h3>
<% notes = Notification.joins(:user) %>
<% notes.each do |f| %>
  <%= f.user.firstname %> :
  <%=f.note%>
  <% if f.user.role == "seller" %>
      <% if f.user.approved == false %>
         <%= button_to "approve", admin_approve_seller_path(f.id, f.user.id), method: :get, class: "btn btn-primary" %>
      <% end %>
  <% end %>
<br>
<% end %>

  