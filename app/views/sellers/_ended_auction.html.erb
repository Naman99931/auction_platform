<h1> Ended Auctions </h1>

<%= @seller_items
          .where("end_time < ?", Time.current)
          .each do |form| %>
  
 <h5> Title : <%= form.title %> </h5>

  <%= image_tag form.images.variant(:thumb) %>
  <br> 
 Item Description : <%= form.item_description %>
  <br>
  Base Price : <%=form.reserved_price %>
  <br>
  Started on : <%= form.start_time %>
  <br>
  Ended on : <%=form.end_time %> 
  <br>

  
<%  end %>